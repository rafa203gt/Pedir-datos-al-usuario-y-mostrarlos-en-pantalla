#include <iostream>
using namespace std;

int main() {
	
	int edad;
	char sexo [10];
	float altura;
	
	cout<<"Di tu edad:"; cin >> edad;
	cout<<"Di tu sexo:"; cin >> sexo;
	cout<<"Di tu altura:"; cin >> altura;
	
	cout << "tu edad es :" << edad << endl;
	cout << "tu sexo es :" << sexo << endl;
	cout << "tu altura es :" << altura << endl;
	
	return 0;
	
}
